#include <iostream>

using namespace std;

int main()
{
  string s;
  cin>>s;
  string s_rev;
  for(int i = s.size() - 1;i>=0;--i)
  {
      s_rev = s_rev + s[i];
  }
  cout<<s_rev<<endl;

    return 0;
}

***********************************************************************************************************************************************************************
reverse a string.
